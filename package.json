{
  "name": "portfolio-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2937",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint './src/**/*.{js,ts,tsx,json}'",
    "e2e": "cross-env APP_ENV=\"./src/tests\" npx playwright test && npx playwright show-report",
    "e2e:actions": "cross-env APP_ENV=\"./src/tests\" npx playwright test --reporter='./playwright-reporter.ts'",
    "snapshot": "cross-env APP_ENV=\"./src/snapshot\" npx playwright test",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:seed": "npx prisma db seed",
    "format": "prettier --write './src/**/*.{js,ts,tsx,json}'",
    "markuplint": "markuplint \"./src/**/*.tsx\""
  },
  "prisma": {
    "seed": "ts-node --compilerOptions {\"module\":\"CommonJS\"} prisma/seeder/seed.ts"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/material-nextjs": "^5.15.11",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.4"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^4.7.6",
    "@markuplint/react-spec": "^4.5.6",
    "@playwright/test": "^1.46.1",
    "@prisma/client": "^5.18.0",
    "@types/highlight.js": "^10.1.0",
    "@types/node": "^20",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "highlight.js": "^11.9.0",
    "html-react-parser": "^5.1.10",
    "markuplint": "^4.9.3",
    "next-auth": "^4.24.7",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "react-hook-form": "^7.53.0",
    "recursive-readdir": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "zod": "^3.23.8"
  }
}
