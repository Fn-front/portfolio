{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "prettier"
  ],
  "rules": {
    "quotes": [2, "single"],
    "jsx-quotes": [2, "prefer-single"],
    "import/no-named-as-default": "off"
